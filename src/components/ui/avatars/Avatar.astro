---
import type { ImageMetadata } from "astro";
import { Image } from "astro:assets";

interface Props {
  src: string,
  alt: string;
}

const { src, alt } = Astro.props;
---

<Image
  class="inline-block h-8 w-8 rounded-full ring-2 ring-neutral-50 dark:ring-zinc-800"
  src={src}
  alt={alt}
  inferSize
  loading={"eager"}
/>
